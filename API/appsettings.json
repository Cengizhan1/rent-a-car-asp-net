{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "RentACar": "Server=(localdb)\\mssqllocaldb; Database=RentACarDB; Trusted_Connection=true"
    },
    "CacheSettings": {
        "SlidingExpiration": 2
    },
    "SeriLogConfigurations": {
        "FileLogConfiguration": {
            "FolderPath": "/logs/"
        },
        "MsSqlConfiguration": {
            "ConnectionString": true,
            "AutoCreateSqlTable": "Server=(localdb)\\mssqllocaldb; Database=RentACarDB; Trusted_Connection=true",
            "TableName":"Logs"
        }
    },
    "AllowedHosts": "*"
}